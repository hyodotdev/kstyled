{
  "name": "babel-plugin-kstyled",
  "version": "0.3.1",
  "description": "Babel plugin for kstyled - Build-time style extraction",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts --clean",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "babel-plugin",
    "react-native",
    "styled-components",
    "stylesheet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hyodotdev/kstyled.git",
    "directory": "packages/babel-plugin-kstyled"
  },
  "homepage": "https://hyodotdev.github.io/kstyled",
  "author": "hyodotdev (https://hyo.dev)",
  "license": "MIT",
  "dependencies": {
    "@babel/helper-module-imports": "^7.25.9",
    "@babel/template": "^7.25.9",
    "@babel/types": "^7.26.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__helper-module-imports": "^7.18.3",
    "@types/babel__template": "^7.4.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  }
}
