{
  "name": "kstyled",
  "version": "0.2.3",
  "description": "Compile-time CSS-in-JS for React Native - styled-components with zero runtime overhead",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --minify",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest --passWithNoTests",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "react-native",
    "styled-components",
    "stylesheet",
    "performance"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hyodotdev/kstyled.git",
    "directory": "packages/kstyled"
  },
  "homepage": "https://hyodotdev.github.io/kstyled",
  "author": "hyodotdev (https://hyo.dev)",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-native": ">=0.64.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
