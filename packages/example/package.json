{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start",
    "dev:safe": "./start-dev.sh",
    "dev:reset": "expo start --reset-cache",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "./start-web.sh",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf .expo node_modules",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:build": "cd android && ./gradlew assembleDebug && cd ..",
    "android:release": "cd android && ./gradlew assembleRelease && cd ..",
    "android:studio": "open -a 'Android Studio' ./android",
    "fix-metro": "./fix-metro.sh",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "logs:android": "adb logcat | grep -E 'ReactNative|Metro|Hermes'"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@emotion/native": "^11.11.0",
    "@emotion/react": "^11.14.0",
    "@expo/metro-runtime": "~6.1.2",
    "expo": "~54.0.0",
    "expo-asset": "~12.0.9",
    "expo-font": "~14.0.9",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "kstyled": "workspace:*",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "babel-plugin-kstyled": "workspace:*",
    "babel-preset-expo": "~54.0.0",
    "eslint": "^9.18.0",
    "typescript": "^5.7.2"
  }
}
